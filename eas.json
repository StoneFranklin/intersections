{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_IOS_BUNDLE_IDENTIFIER": "com.stonefranklin.intersections",
        "EXPO_ANDROID_PACKAGE": "com.stonefranklin.intersections"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "autoIncrement": true
      },
      "env": {
        "EXPO_IOS_BUNDLE_IDENTIFIER": "com.stonefranklin.intersections",
        "EXPO_ANDROID_PACKAGE": "com.stonefranklin.intersections"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
